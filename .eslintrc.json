{
  "parser": "@babel/eslint-parser",
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "import/no-extraneous-dependencies": [
    "error",
    {
      "devDependencies": ["**/*.test.ts", "**/*.test.tsx"]
    }
  ],
  "rules": {
    "linebreak-style": "off",
    "import/extensions": "off"
  }
}
